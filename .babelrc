{
  "presets": [
      [
          "env",
          {
              "modules": false,
              "targets": {
                  "browsers": [
                      "> 1%",
                      "last 2 versions",
                      "not ie <= 8"
                  ]
              }
          }
      ],
      "stage-2",
      "react"
  ],
    "comments": false,
    "plugins": [
        ["transform-decorators-legacy"],
        [
            "transform-runtime"
        ],
        [
            "import", {
                "libraryName": "antd-mobile",
                "style": true
            }
        ]
    ],

    "env": {
        "production": {
            "plugins": [[
                "transform-react-remove-prop-types",
                {
                    "removeImport": true,
                    "ignoreFilenames": ["node_modules"]
                }
            ]]
        },
        "development": {
            "plugins": ["react-hot-loader/babel"]
        },
        "test": {
            "presets": ["env", "stage-1"],
            "plugins": [
                ["module-resolver", {
                    "alias": {
                        "src": "./src"
                    }
                }]
            ]
        }
    }
}
